{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "lib": [ "es2015", "dom" ],
    "module": "commonjs",
    "removeComments": true,
    "sourceMap": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "fb",
      "isomorphic-fetch",
      "node",
      "react-helmet",
      "systemjs",
      "webpack"
    ]
  },
  "exclude": [
    "node_modules",
    "build",
    "webpack"
  ],
  "awesomeTypescriptLoaderOptions": {},
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": { "rewriteTsconfig": false }
}