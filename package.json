{
  "name": "phreshr-dev",
  "version": "0.0.1",
  "description": "React starter for building universal apps with Typescript, Webpack 2, Apollo",
  "scripts": {
    "analyze": "webpack --config webpack/webpack.client.ts --profile --json > stats.json",
    "lint": "eslint . --ext js,gql,graphql",
    "persistgraphql": "persistgraphql ui --add_typename",
    "generate-graphql-types": "apollo-codegen generate ui/graphql/**/*.graphql --schema schema.json --target ts --output ui/graphql/schema.ts",
    "generate-graphql-documents": "cd ./ui/graphql; graphql-document-collector **/*.graphql > documents.json",
    "prebuild": "rimraf build/*",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --progress --config webpack/webpack.client.ts",
    "build:server": "webpack --progress --config webpack/webpack.server.ts",
    "start": "node build/server.js",
    "predev": "npm run build",
    "dev": "nodemon build/server.js"
  },
  "dependencies": {
    "animejs": "^2.0.2",
    "body-parser": "^1.17.1",
    "cloudinary-react": "^1.0.3",
    "compression": "1.6.2",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "4.15.2",
    "firebase": "^3.8.0",
    "graphql-anywhere": "^3.0.1",
    "graphql-tag": "^2.0.0",
    "history": "^4.6.1",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.0.2",
    "morgan": "1.8.1",
    "persistgraphql": "^0.3.1",
    "picturefill": "^3.0.2",
    "polished": "^1.0.4",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-apollo": "^1.0.0",
    "react-dom": "^15.5.4",
    "react-ga": "^2.2.0",
    "react-helmet": "^5.0.3",
    "react-motion": "^0.4.8",
    "react-router-dom": "^4.1.1",
    "react-share": "^1.13.2",
    "react-social-icons": "^2.6.0",
    "recompose": "^0.23.1",
    "serialize-javascript": "^1.3.0",
    "styled-components": "2.0.0-15",
    "subscriptions-transport-ws": "^0.5.5",
    "video.js": "^6.0.1",
    "videojs-chromecast": "^2.0.8",
    "videojs-contrib-ads": "^4.2.6",
    "videojs-dock": "^2.1.1",
    "videojs-playlist": "^3.1.0",
    "videojs-playlist-ui": "^3.0.3",
    "webfontloader": "^1.6.27"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.3",
    "@types/compression": "0.0.33",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/fb": "^0.0.22",
    "@types/history": "^4.5.0",
    "@types/isomorphic-fetch": "0.0.33",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.13",
    "@types/react": "^15.0.23",
    "@types/react-dom": "^15.5.0",
    "@types/react-helmet": "^5.0.1",
    "@types/react-redux": "^4.4.39",
    "@types/redux": "^3.6.0",
    "@types/systemjs": "^0.20.2",
    "@types/videojs": "^5.16.2",
    "@types/webpack": "^2.2.15",
    "apollo-codegen": "^0.10.11",
    "assets-webpack-plugin": "3.5.1",
    "awesome-typescript-loader": "^3.1.2",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.4.0",
    "babili-webpack-plugin": "^0.0.11",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.19.0",
    "eslint-plugin-graphql": "^0.7.0",
    "file-loader": "0.11.1",
    "graphql-document-collector": "^0.1.3",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "react-hot-loader": "3.0.0-beta.6",
    "redbox-react": "^1.3.6",
    "rimraf": "2.6.1",
    "source-map-loader": "^0.2.1",
    "source-map-support": "^0.4.14",
    "sw-precache-webpack-plugin": "^0.9.1",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typescript": "2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "1.5.4"
  }
}
